<?xml version="1.0" encoding="utf-8"?>
<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:drawable="@drawable/ic_claims"
    tools:ignore="NewApi">
    <target
        android:animation="@anim/left_animator"
        android:name="left" />
    <target
        android:animation="@anim/animator"
        android:name="up" />
    <target
        android:animation="@anim/animator_from_right"
        android:name="down" />

    <target
        android:animation="@anim/right_animator"
        android:name="right" />

</animated-vector>